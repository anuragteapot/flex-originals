    "nodemon": "^2.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "url-loader": "^3.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "src/*",
      "build/*",
      "dist/*"
    ],
    "delay": "2500"
  }
}
