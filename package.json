{
  "name": "FlexOriginals",
  "version": "1.0.0",
  "description": "A video and audio streaming application.",
  "author": "Anurag Kumar",
  "license": "MIT",
  "main": "server/server.js",
  "scripts": {
    "start:staging": "cross-env NODE_ENV=production MONGODB_HOST=localhost node . & cross-env NODE_ENV=production PORT=3333 node client",
    "start:prod": "cross-env NODE_ENV=production node . & cross-env NODE_ENV=production PORT=3333 node client & cross-env NODE_ENV=production PORT=3355 node cdn",
    "staging": "npm run build && cross-env NODE_ENV=production npm run start:staging",
    "prod": "npm run build && npm run start:prod",
    "dev": "cross-env NODE_ENV=development nodemon node . & cross-env NODE_ENV=development node client & cross-env NODE_ENV=development node cdn",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --hide-modules",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "test:watch": "jest --watch",
    "lb-start": "nodemon",
    "posttest": "npm run lint && nsp check"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "aws-sdk": "^2.538.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.1.0",
    "helmet": "^3.10.0",
    "loopback": "^3.22.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.2.0",
    "loopback-connector-mongodb": "^5.0.1",
    "lru-cache": "^5.1.1",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ora": "^4.0.3",
    "randomstring": "^1.1.5",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^3.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "underscore": "^1.9.1",
    "url": "^0.11.0",
    "vue": "^2.6.10",
    "vue-meta": "^2.2.1",
    "vue-router": "^3.1.2",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
    "vuex-router-sync": "^5.0.0"
